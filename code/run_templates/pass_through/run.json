{
  "steps": [
    {
      "id": "fetch_parameters",
      "command": "csm-data",
      "arguments": [
        "api",
        "run-load-data"
      ],
      "useSystemEnvironment": true
    },
    {
      "id": "generate_output_data",
      "command": "python",
      "arguments": [
        "code/run_templates/pass_through/generate_csv_files.py"
      ],
      "precedents": [
        "fetch_parameters"
      ],
      "useSystemEnvironment": true
    },
    {
      "id": "load_results_to_store",
      "command": "csm-data",
      "arguments": [
        "store",
        "load-csv-folder"
      ],
      "precedents": [
        "generate_output_data"
      ],
      "useSystemEnvironment": true
    },
    {
      "id": "send_results_output",
      "command": "csm-data",
      "arguments": [
        "store",
        "output"
      ],
      "precedents": [
        "load_results_to_store"
      ],
      "useSystemEnvironment": true
    }
  ]
}